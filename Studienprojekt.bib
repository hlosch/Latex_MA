issn = {14698366},
journal = {Higher Education Research and Development},
keywords = {adaptation to tertiary study,motivation,student retention,student well-being},
number = {2},
pages = {201--216},
title = {{The influence of motivation and adaptation on students' subjective well-being, meaning in life and academic performance}},
volume = {35},
year = {2016}
}
