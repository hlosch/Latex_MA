keywords = {80 and over,Adult,Aged,Andrea Creech,Creativity,Female,Health Status*,Helena Gaunt,Humans,MEDLINE,Male,Mental Health*,Middle Aged,Music / psychology*,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,Personal Autonomy,Principal Component Analysis,PubMed Abstract,Quality of Life,Research Support,Self Concept,Susan Hallam,doi:10.1177/1757913912466950,pmid:23308006},
